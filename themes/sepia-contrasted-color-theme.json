{
    "name": "Sepia contrasted",
    "type": "light",
    "tokenColors": [
        {
            "settings": {
                "foreground": "#5E6E75"
            }
        },
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded"
            ],
            "settings": {
                "foreground": "#5E6E75"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#0E7D15"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#686868"
            }
        },
        {
            "name": "Regexp",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#D30102"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#C32A80"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable.language",
                "variable.other"
            ],
            "settings": {
                "foreground": "#0A4D9B"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#5B4636"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#073642"
            }
        },
        {
            "name": "Class name",
            "scope": [
                "entity.name.class",
                "entity.name.type",
                "entity.name.namespace",
                "entity.name.scope-resolution"
            ],
            "settings": {
                "foreground": "#0A4D9B"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#0A4D9B"
            }
        },
        {
            "name": "Variable start",
            "scope": "punctuation.definition.variable",
            "settings": {
                "foreground": "#5B4636"
            }
        },
        {
            "name": "Embedded code markers",
            "scope": [
                "punctuation.section.embedded.begin",
                "punctuation.section.embedded.end"
            ],
            "settings": {
                "foreground": "#D30102"
            }
        },
        {
            "name": "Built-in constant",
            "scope": [
                "constant.language",
                "meta.preprocessor"
            ],
            "settings": {
                "foreground": "#7E5800"
            }
        },
        {
            "name": "Support.construct",
            "scope": [
                "support.function.construct",
                "keyword.other.new"
            ],
            "settings": {
                "foreground": "#D30102"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#CB4B16"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {}
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {}
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#0A4D9B"
            }
        },
        {
            "name": "Tag start/end",
            "scope": [
                "punctuation.definition.tag.begin",
                "punctuation.definition.tag.end"
            ],
            "settings": {
                "foreground": "#5C6F70"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#C32A80"
            }
        },
        {
            "name": "CSS class name",
            "scope": "entity.other.attribute-name.class.css",
            "settings": {
                "foreground": "#0A4D9B"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "foreground": "#0A4D9B"
            }
        },
        {
            "name": "Continuation",
            "scope": "punctuation.separator.continuation",
            "settings": {
                "foreground": "#D30102"
            }
        },
        {
            "name": "Library constant",
            "scope": [
                "support.constant",
                "support.variable"
            ],
            "settings": {}
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#5B4636"
            }
        },
        {
            "name": "Library Exception",
            "scope": "support.type.exception",
            "settings": {
                "foreground": "#CB4B16"
            }
        },
        {
            "name": "CSS property name",
            "scope": "support.type.property-name.css",
            "settings": {
                "foreground": "#893789"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {}
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "foreground": "#cd3131"
            }
        },
        {
            "name": "diff: header",
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#0A4D9B"
            }
        },
        {
            "name": "diff: deleted",
            "scope": "markup.deleted",
            "settings": {
                "fontStyle": "",
                "foreground": "#dc322f"
            }
        },
        {
            "name": "diff: changed",
            "scope": "markup.changed",
            "settings": {
                "fontStyle": "",
                "foreground": "#cb4b16"
            }
        },
        {
            "name": "diff: inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#1F7777"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#5B4636"
            }
        },
        {
            "name": "Markup Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#7E5800"
            }
        },
        {
            "name": "Markup Styling",
            "scope": [
                "markup.bold",
                "markup.italic"
            ],
            "settings": {
                "foreground": "#C32A80"
            }
        },
        {
            "name": "Markup: Strong",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Emphasis",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "fontStyle": "",
                "foreground": "#1F7777"
            }
        },
        {
            "name": "Markup Headings",
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0A4D9B"
            }
        },
        {
            "name": "Markup Setext Header",
            "scope": "markup.heading.setext",
            "settings": {
                "fontStyle": "",
                "foreground": "#0A4D9B"
            }
        }
    ],
    "colors": {
        "focusBorder": "#C32A80",
        "input.background": "#DDD6C1",
        "input.foreground": "#212324",
        "input.placeholderForeground": "#4B5E64",
        "inputOption.activeBorder": "#D3AF86",
        "badge.background": "#B58900AA",
